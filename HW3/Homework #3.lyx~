\begin_layout Paragraph
Problem 5.
\end_layout

\begin_layout Enumerate
\begin_inset Formula $Sensitivity=\frac{TruePositive}{TruePositive+FalseNegative}$
\end_inset


\end_layout

\begin_deeper
\begin_layout Standard
\begin_inset Formula $Specificity=\frac{TrueNegative}{TrueNegative+FalsePositive}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $Probability(PersonEnjoysJoke|PersonEnjoysPinacoladas)=\frac{(P(EnjoysPinacoladas|EnjoysJoke)*P(EnjoysJoke))}{P(EnjoysPinacoladas)}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $D=PersonEnjoysJoke$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $S=PersonEnjoysPinacoladas$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $P(D|S)=\frac{(P(S|D)*P(D))}{P(S)}$
\end_inset


\end_layout

\begin_layout Standard
and from the lecture notes, it follows that
\end_layout

\begin_layout Standard
\begin_inset Formula $P(D|S)=\frac{P(S|D)*P(D)}{P(S|D)*P(D)+[1-P(\neg S|\neg D)][1-P(D)]}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $P(D|S)=\frac{Sensitivity*Prevalence}{(Sensitivity*Prevalence)+(1-Specificity)*(1-Prevalence)}$
\end_inset

 
\end_layout

\begin_layout Standard
\begin_inset Formula $P(D|S)=\frac{.999*P(D)}{.999*P(D)+(1-.90)*(1-P(D)}=P(T2|T1)$
\end_inset

 
\end_layout

\end_deeper
\begin_layout Enumerate
\begin_inset Formula $P(DS|T1*T2)$
\end_inset


\end_layout

